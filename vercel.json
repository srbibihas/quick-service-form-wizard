
{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "DODO_API_KEY": "@dodo_api_key",
    "DODO_WEBHOOK_SECRET": "@dodo_webhook_secret",
    "NEXT_PUBLIC_URL": "@next_public_url"
  }
}
